{
    "title": "文档中心",
    "author": "Joe Chan",
    "language": "zh-hans",
    "gitbook": ">=2.0.0",
    "plugins": [
        "theme-fexa",
        "-lunr",
        "-search",
        "search-pro",
        "splitter",
        "code",
        "copy-code-button",
        "expandable-chapters",
        "expandable-chapters-small",
        "yahei",
        "accordion",
        "popup"
    ],
    "pluginsConfig": {
        "theme-fexa": {
            "logo": "/logo.png"
        },
        "summary": {
            "excludeDir": [
                "img",
                "styles"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "fontSettings": {
            "family": "msyh",
            "size": 2
        }
    }
}